<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="eus.ehu.ridesfx.uicontrollers.QueryReservationsController"
            prefHeight="400.0" prefWidth="600.0" styleClass="border-pane" stylesheets="@Styles.css">
    <children>
    <Label layoutX="55.0" layoutY="14.0" styleClass="bold-label" text="RESERVATIONS">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
        <Button layoutX="285.0" layoutY="269.0" mnemonicParsing="false" onAction="#closeClick" styleClass="bold-label" text="Back" />

        <TableView fx:id="alertTable" layoutX="55.0" layoutY="56.0" prefHeight="175.0" prefWidth="463.0">
            <columns>
                <TableColumn fx:id="departC" prefWidth="96.0" text="DepartCity" />
